{
  "family": "spacex-web-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::456276248619:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::456276248619:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "spacex-web",
      "image": "456276248619.dkr.ecr.us-east-1.amazonaws.com/spacex-web:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "NEXT_TELEMETRY_DISABLED",
          "value": "1"
        }
      ],
      "secrets": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/spacex-web",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
